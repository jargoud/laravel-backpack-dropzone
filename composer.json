{
    "name": "jargoud/laravel-backpack-dropzone",
    "description": "Dropzone field for Laravel Backpack",
    "keywords": [
        "jargoud",
        "laravel-backpack-dropzone"
    ],
    "homepage": "https://github.com/jargoud/laravel-backpack-dropzone",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jérémy Argoud",
            "email": "jeremy.argoud@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "illuminate/support": "^11.0",
        "pion/laravel-chunk-upload": "^1.5"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Jargoud\\LaravelBackpackDropzone\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jargoud\\LaravelBackpackDropzone\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jargoud\\LaravelBackpackDropzone\\Providers\\LaravelBackpackDropzoneServiceProvider"
            ]
        }
    }
}
